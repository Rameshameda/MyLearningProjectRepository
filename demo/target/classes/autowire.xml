<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="shirt" class="com.example.demo.autowire.Shirt">
		<property name="shirt" value="us-polo"></property>
	</bean>
	<bean id="phant" class="com.example.demo.autowire.Phant">
		<property name="phant" value="peter england"></property>
	</bean>
	<bean id="blazer" class="com.example.demo.autowire.Blazer">
		<property name="blazer" value="arvind"></property>
	</bean>
	<bean id="suite" class="com.example.demo.autowire.Suite" autowire="byName"/>
	
</beans>